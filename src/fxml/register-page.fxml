<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="524.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.RegisterPage">
   <children>
      <Button fx:id="registerButton" layoutX="111.0" layoutY="437.0" mnemonicParsing="false" onAction="#Register" text="Register" />
      <TextField fx:id="passwordField" layoutX="71.0" layoutY="140.0" promptText="Password" />
      <TextField fx:id="nameField" layoutX="72.0" layoutY="189.0" promptText="First name" />
      <TextField fx:id="lastnameField" layoutX="72.0" layoutY="237.0" promptText="Last name" />
      <TextField fx:id="phoneField" layoutX="71.0" layoutY="283.0" promptText="Phone number" />
      <DatePicker fx:id="birthField" layoutX="71.0" layoutY="333.0" prefHeight="25.0" prefWidth="149.0" promptText="Date of birth" />
      <TextField fx:id="loginField" layoutX="71.0" layoutY="91.0" promptText="Login" />
      <CheckBox fx:id="adminField" layoutX="301.0" layoutY="287.0" mnemonicParsing="false" text="I am an admin" />
      <CheckBox fx:id="medicalCheck" layoutX="275.0" layoutY="95.0" mnemonicParsing="false" text="Have valid medical card" />
      <CheckBox fx:id="managerCheck" layoutX="72.0" layoutY="382.0" mnemonicParsing="false" onAction="#disableFields" text="Manager" />
      <TextField fx:id="distanceField" layoutX="275.0" layoutY="165.0" promptText="Full driven distance" />
      <CheckBox fx:id="licenceCheck" layoutX="274.0" layoutY="132.0" mnemonicParsing="false" text="Have valid driver licence" />
      <Button layoutX="295.0" layoutY="437.0" mnemonicParsing="false" onAction="#getBackToLogin" text="Get back to login" />
   </children>
</AnchorPane>
